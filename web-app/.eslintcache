rorMessage('Error servor, please try again')\r\n            console.log(err)\r\n        })\r\n    }\r\n    return (\r\n        <div>\r\n            <StyledForm onSubmit={(e) =>submit(e)}>\r\n            <h1>Login</h1>\r\n            <StyledDiv>\r\n                <StyledLabel>Username : </StyledLabel>\r\n                <StyledInput type='text' onChange={e =>setFormState({...formState, username:e.target.value})}></StyledInput>\r\n            </StyledDiv>\r\n            <StyledDiv>\r\n                <StyledLabel>Password : </StyledLabel>\r\n                <StyledInput type='password'onChange={e =>setFormState({...formState, password:e.target.value})}></StyledInput>\r\n            </StyledDiv>\r\n            <StyledLabelError>{errorMessage}</StyledLabelError>\r\n            <StyledInput type='submit'></StyledInput>\r\n            </StyledForm>\r\n        </div>\r\n    )\r\n}\r\nconst StyledForm = styled.form`\r\ndisplay:flex;\r\nflex-direction: column;\r\nalign-items: center;\r\njustify-content:center;\r\n`\r\nconst StyledDiv = styled.div`\r\nwidth:auto;\r\nheight:auto;\r\n`\r\nconst StyledLabel = styled.label`\r\ndisplay: inline-block;\r\nwidth: 100px;\r\ntext-align: justify;\r\nfont-weight:bold;\r\n`\r\nconst StyledLabelError = styled.label`\r\nwidth: auto;\r\ntext-align: justify;\r\nfont-weight:bold;\r\ncolor:red;\r\n`\r\nconst StyledInput = styled.input`\r\nmargin: 6px 0px;\r\nborder-radius: 12px;\r\nborder: none;\r\nbackground-color:#1eaf7a;\r\nheight:30px;\r\ncolor:white;\r\n`\r\nexport default Login;","C:\\Users\\dodav\\Documents\\Formation\\Majeur2\\Projets React\\shinsekai\\web-app\\src\\screens\\home.js",["67","68"],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":1,"column":8,"nodeType":"75","messageId":"76","endLine":1,"endColumn":12},{"ruleId":"77","severity":1,"message":"78","line":5,"column":1,"nodeType":"79","endLine":8,"endColumn":3},{"ruleId":"69","replacedBy":"80"},{"ruleId":"71"